<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Permisos de acceso para el modelo certificado.seguridad -->
        <record id="access_cedula_extractor_user" model="ir.model.access">
            <field name="name">certificado.seguridad.user</field>
            <field name="model_id" search="[('model','=','certificado.seguridad')]"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read">True</field>
            <field name="perm_write">True</field>
            <field name="perm_create">True</field>
            <field name="perm_unlink">False</field>
        </record>

        <record id="access_certificado_seguridad_manager" model="ir.model.access">
            <field name="name">certificado.seguridad.manager</field>
            <field name="model_id" search="[('model','=','certificado.seguridad')]"/>
            <field name="group_id" ref="base.group_erp_manager"/>
            <field name="perm_read">True</field>
            <field name="perm_write">True</field>
            <field name="perm_create">True</field>
            <field name="perm_unlink">True</field>
        </record>
    </data>
</odoo>